<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="login2" namespace="/" extends="default">
		<action name="showClaimVoucher" method="showClaimVoucher"
			class="com.action.ClaimVoucherAction">
			<!-- <result type="redirect">/jsp/claim/claim_voucher_search.jsp</result> -->
			<result name="success">/jsp/claim/claim_voucher_search.jsp</result>
			<result name="input">error.jsp</result>
		</action>

		<action name="showClaimVoucherByCondition" method="showClaimVoucherByCondition"
			class="com.action.ClaimVoucherAction">
			<!-- <result type="redirect">/jsp/claim/claim_voucher_search.jsp</result> -->
			<result name="success">/jsp/claim/claim_voucher_search.jsp</result>
			<result name="input">error.jsp</result>
		</action>

		<action name="searchByPaging" method="searchByPaging"
			class="com.action.ClaimVoucherAction">
			<result name="success">/jsp/claim/claim_voucher_search.jsp</result>
			<result name="input">error.jsp</result>
		</action>

		<action name="addClaimVoucher" method="addClaimVoucher"
			class="com.action.ClaimVoucherAction">
			<result name="success">/jsp/claim/claim_voucher_add.jsp</result>
			<result name="input">error.jsp</result>
		</action>
	</package>
</struts>
